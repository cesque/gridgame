pico-8 cartridge // http://www.pico-8.com
version 14
__lua__
-- test --
grid = {}
turning = {}
width = 15
height = 14

turnRate = 0.085

mouseLast = false
mouseX = 0
mouseY = 0
mouseL = 0

score = 0

function _init()
  poke(0x5F2D, 1) -- enable mouse
  for x = 0, width do 
    grid[x] = {}
    turning[x] = {}
    for y = 0, height do
      grid[x][y] = 0
      turning[x][y] = 0
    end
  end

  randomize()
end

function randomize()
  for x = 0, width do 
    for y = 0, height do
      grid[x][y] = flr(rnd(4))
      turning[x][y] = 0
    end
  end
  for x = 0, width do 
    for y = 0, height do
      propagate(x, y)
    end
  end
  score = 0
end

function activate(x, y)
  turning[x][y] = 0.001
  score += 10
  sfx(flr(rnd(5)),flr(rnd(2)))
end

function propagate(x, y) 
  r = grid[x][y]
  if r == 0 then
    if (x+1) <= width and (grid[x+1][y+0] == 1 or grid[x+1][y+0] == 2) then activate(x+1,y+0) end
    if (y+1) <= height and (grid[x+0][y+1] == 2 or grid[x+0][y+1] == 3) then activate(x+0,y+1) end
  end
  if r == 1 then
    if (x-1) >= 0 and (grid[x-1][y+0] == 0 or grid[x-1][y+0] == 3) then activate(x-1,y+0) end
    if (y+1) <= height and (grid[x+0][y+1] == 2 or grid[x+0][y+1] == 3) then activate(x+0,y+1) end
  end
  if r == 2 then
    if (x-1) >= 0 and (grid[x-1][y+0] == 0 or grid[x-1][y+0] == 3) then activate(x-1,y+0) end
    if (y-1) >= 0 and (grid[x+0][y-1] == 0 or grid[x+0][y-1] == 1) then activate(x+0,y-1) end
  end
  if r == 3 then
    if (x+1) <= width and (grid[x+1][y+0] == 1 or grid[x+1][y+0] == 2) then activate(x+1,y+0) end
    if (y-1) >= 0 and (grid[x+0][y-1] == 0 or grid[x+0][y-1] == 1) then activate(x+0,y-1) end
  end
end

function _update60()
  if btnp(4) then
    randomize()
  end

  mouseX = stat(32)
  mouseY = stat(33)
  mouseL = (stat(34) % 2) == 1
  -- do stuff
  for x = 0, width do 
    for y = 0, height do
      if turning[x][y] > 0 then
        turning[x][y] += turnRate
        if turning[x][y] >= 1 then
          turning[x][y] = 0
          grid[x][y] = (grid[x][y] + 1) % 4
          propagate(x,y)
        end
      end
    end
  end

  if mouseL and (not mouseLast) then
    posX = flr(mouseX / 8)
    posY = flr(mouseY / 8)
    if posX >= 0 and posX <= width and posY >= 0 and posY <= height then
      if turning[posX][posY] == 0 then turning[posX][posY] = 0.001 end
    end
  end
  -- end
  mouseLast = mouseL
end

function _draw()
  cls(1)
  for x = 0, width do 
    for y = 0, height do
      rot = grid[x][y]
      --flipX = rot == 1 or rot == 2
      --flipY = rot == 2 or rot == 3
      gradation = flr(turning[x][y] * 4)
      if(turning[x][y] > 0) then pal(12, 7) end
      if(turning[x][y] > 0.5) then pal(12, 6) end
      spr((rot * 4) + gradation, x*8, y*8)
      pal()
    end
  end
  spr(mouseL and 17 or 16, mouseX, mouseY)
  line(0, 120, 128, 120, 12)
  print(score, 128-(#(score.."")*4 + 1), 122, 7)
  print("z to restart", 1, 122, 7)
end
